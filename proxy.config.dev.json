{
   "/api/**": {
        "target": "http://laroute.ddns.net:8088/",
        "secure": false,
        "changeOrigin": true,
        "logLevel": "info",
        "pathRewrite": {
            "^/api": ""
        }
    },
    "/ressources/**":  {
        "target": "https://ressources-laroute.ddns.net:8080/",
        "secure": true,
        "changeOrigin": true,
        "logLevel": "info",
        "pathRewrite": {
            "^/ressources": ""
        }
    },
    "/keycloak/**": {
        "target": "https://laroute.ddns.net:8443/",
        "secure": true,
        "changeOrigin": true,
        "logLevel": "info",
        "pathRewrite": {
            "^/keycloak": ""
        }
    },
    "/img/**": {
        "target": "https://ressources-laroute.ddns.net:81/",
        "secure": true,
        "changeOrigin": true,
        "logLevel": "info",
        "pathRewrite": {
            "^/img": ""
        }
    }
}