{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "perso_frontend": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/perso_frontend",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "*",
                "input": "src/public",
                "output": "/"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/cyan-orange.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "src/assets/css/styles.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/media.css",
              "src/assets/css/theme.css",
              {
                "input": "src/app/profil/profil.component.css",
                "inject":true,
                "bundleName": "profil"
              },
              {
                "input": "src/app/profil/upload-image/upload-image.component.css",
                "inject":true,
                "bundleName": "upload-image"
              }
            ],
            "scripts": ["src/assets/js/jquery-1.11.0.min.js","src/assets/js/jquery.sudoSlider.min.js","src/assets/js/global.js","src/assets/js/modernizr.js"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
              "proxyConfig": "proxy.config.json",
              "allowedHosts": ["ressources-laroute.ddns.net"],
              "ssl": true,
              "sslCert": "/etc/letsencrypt/live/ressources-laroute.ddns.net/fullchain.pem",
              "sslKey": "/etc/letsencrypt/live/ressources-laroute.ddns.net/privkey.pem"
          },
          "configurations": {
            "production": {
              "buildTarget": "perso_frontend:build:production"
            },
            "development": {
              "buildTarget": "perso_frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/cyan-orange.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "src/assets/css/styles.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/media.css",
              "src/assets/css/theme.css",
              {
                "input": "src/app/profil/profil.component.css",
                "inject": true,
                "bundleName": "profil"
              }
            ],
            "scripts": ["src/assets/js/jquery-1.11.0.min.js","src/assets/js/jquery.sudoSlider.min.js","src/assets/js/global.js","src/assets/js/modernizr.js"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
